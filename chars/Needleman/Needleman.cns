;---------------------------------------------------------------------------
; RUN_FWD
[Statedef 100]
type    = S
physics = N
anim = 100
sprpriority = 1
velset = ifelse(!AIlevel, 6, 0), 0
ctrl = ifelse(Ailevel, 0, 1)

[State 100, 1] ;Prevent run from canceling into walk
