;---------------------------------------------------------------------------------------------
;Introduction Decider

;If you add more [statedef]s as intros, make sure those states
;are added as triggers in [statedef -2] for "Stop Lifebar sound"
;and "Starting Lifebar"

;All intro states are to changestate into [statedef 193] which
;contains the code for the character to move around before the
;match and to have their lifebar be filled up in the traditional
;Megaman style. (Only Team 2 on the right has the lifebar FX)

[Statedef 190]
type = S

[State 190, Life is Empty]
type = LifeSet
trigger1 = TeamSide = 2
value = 1

[State 190, 3]
type = ChangeState
trigger1 = enemy,name = "Roll" || enemy,name = "Roll RMM"
value = 191

[State 190, 3]
type = ChangeState
trigger1 = enemy,name = "Bass" || enemy,name = "Bass RMM"
value = 192

[State 190, 3]
type = ChangeState
trigger1 = enemy,name = "Rockman" || enemy,name = "Rock"
trigger2 = enemy,name = "Mega Man" || enemy,name = "Megaman" || enemy,name = "Mega Man RMM"
value = 193

[State 190, 3]
type = ChangeState
trigger1 = enemy,name = "Blues" || enemy,name = "Blues RMM"
trigger2 = enemy,name = "Protoman" || enemy,name = "Proto Man" || enemy,name = "Proto Man RMM"
value = 194

[State 190, 3]
type = ChangeState
trigger1 = enemy,name = "Rush" || enemy,name = "Rush RMM"
value = 196

[State 190, 2]
type = ChangeState
trigger1 = 1
value = 191+random%2

