;Non-Intrusive Armor_(***code used from ShinRa358!!***)
[Statedef 9393]
type = U
movetype = I ;<---idle status
physics = N
ctrl = 0

[State 9393, BindToRoot]
type = BindToRoot ;<---binds this helper to the player.
trigger1 = 1
ignorehitpause = 1

[State 9393, ChangeAnim] ;<---copycat's YOU, the player's, animation. Root = YOU,
type = ChangeAnim           ;Parent = Helper that spawns another helper.
trigger1 = selfanimexist(root,anim)
elem = root,animelemno(0)
value = root,anim

[State 9393, HitBy]
type = HitBy ;<----This helper can only be hit by the below parameters
trigger1 = 1
value = SCA,AA,AP

[State 9393, AssertSpecial]
type = AssertSpecial
trigger1 = 1
flag = invisible
flag2 = noshadow ;<----shadows don't appear when something is invisble but i put
                      ;this trigger here anyway.

[State 9393, HitOverride]
type = HitOverride ;<----Changes Helper state 9393 to 9394 if hit by the below parameters.
trigger1 = 1
attr = SCA,AA,AP
stateno = 9394
time = -1
slot = 0

[State 9393, DestroySelf]
type = DestroySelf
trigger1 = root,var(3)!=1 ;<----Mode variable goes here. If Armor is active all the time or if you do not have a mode var, keep this sctrl nulled.


