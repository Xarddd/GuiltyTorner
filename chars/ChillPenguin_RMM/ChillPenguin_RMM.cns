elem = root,animelemno(0)
value = root,anim

[State 9394, NotHitBy]
type = NotHitBy
trigger1 = 1
value = SCA

[State 9394, AssertSpecial]
type = AssertSpecial
trigger1 = 1
flag = invisible
flag2 = noshadow

[State 9394, ChangeState]
type = ChangeState ;Solidifies single hit attacks from doing multiple hits
trigger1 = 1
value = 9393

[State 9394, DestroySelf]
type = DestroySelf
trigger1 = !(enemynear,movehit) ;<---gets rid of this helper state after enemy attack lands with some space in between to prevent a single hit from happening more than once. Should be fine as is. Thanx to Cyanide for this portion.
trigger2 = (root,var(3)!= 1) ;<----Mode variable goes here. If Armor is active all the time or if you do not have a mode var, keep this trigger nulled.


